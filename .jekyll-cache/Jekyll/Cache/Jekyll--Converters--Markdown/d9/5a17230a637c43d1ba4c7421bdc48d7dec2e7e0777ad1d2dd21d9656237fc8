assume cs:codesg, ds:data, es:table data segment
	db '1975', '1976', '1977', '1978', '1979', '1980', '1981', '1982', '1983'
	db '1984', '1985', '1986', '1987', '1988', '1989', '1990', '1991', '1992'
	db '1993', '1994', '1995'
; ä»¥ä¸Šæ˜¯è¡¨ç¤º21å¹´çš„21ä¸ªå­—ç¬¦ä¸²

	dd 16, 22, 382, 1356, 2390, 8000, 16000, 24486, 50065, 97479, 140417, 197514
	dd 345980, 590827, 803530, 1183000, 1843000, 2759000, 3753000, 4649000, 5937000
; ä»¥ä¸Šæ˜¯è¡¨ç¤º21å¹´å…¬å¸æ€»æ”¶å…¥çš„21ä¸ªdwordå‹æ•°æ®

	dw 3, 7, 9, 13, 28, 38, 130, 220, 476, 778, 1001, 1442, 2258, 2793, 4037, 5635, 8226
	dw 11542, 14430, 15257, 17800
; ä»¥ä¸Šæ˜¯è¡¨ç¤º21å¹´å…¬å¸é›‡å‘˜äººæ•°çš„21ä¸ªwordå‹æ•°æ®
data ends

table segment
	db 21 dup ('year summ ne ?? ')
table ends

codesg segment

	start: mov ax, data
				 mov ds, ax
				 mov ax, table
				 mov es, ax
	
				 mov bx, 0
				 mov si, 0
				 mov di, 0
				 mov cx, 21
	
	    s: mov al, [bx]      ;å¯¹å¹´ä»½è¿›è¡Œmov
			   mov es:[di], al
				 mov al, [bx+1]
				 mov es:[di+1], al
				 mov al, [bx+2]
				 mov es:[di+2], al
				 mov al, [bx+3]
				 mov es:[di+3], al
	
				 mov ax, 54H[bx]   ;å¯¹æ”¶å…¥è¿›è¡Œmovï¼Œæ•°æ®ç±»å‹æ˜¯ddï¼Œæ‰€ä»¥åˆ†å¼€ä¸¤ä¸ªé€šç”¨å¯„å­˜å™¨
				 mov dx, 56H[bx]
				 mov es:5h[di], ax  ;æ³¨æ„ä¸Šç¬¬ä¸‰å›¾ä¸­çš„ç©ºæ ¼
				 mov es:7h[di], dx  ;ä½ä½å­˜æ”¾åœ¨ä½ä½ï¼Œé«˜ä½æ•°æ®å­˜æ”¾åœ¨é«˜ä½
	
				 mov ax, 0A8H[si]  ;å¯¹é›‡å‘˜æ•°è¿›è¡Œmov
				 mov es:0AH[di], ax
	
				 mov ax, 54h[bx]  ;å·¥èµ„æ€»æ•°é™¤ä»¥é›‡å‘˜
				 mov dx, 56h[bx]
				 div word ptr ds:0A8H[si]
				 mov es:0dh[di], ax  ;é»˜è®¤å•†ä¿å­˜åœ¨axä¸­ï¼Œä¸ä¿ç•™ä½™æ•°
	
				 add bx, 4    ;æŒ‡é’ˆ4ä¸ªå­—èŠ‚
				 add si, 2    ;2ä¸ªå­—èŠ‚
				 add di, 16    ;æŒ‡é’ˆè¡Œ
					
 loop s

mov ax, 4c00h
int 21h

codesg ends
end start

</code></pre>

